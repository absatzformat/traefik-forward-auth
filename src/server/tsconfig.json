{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "rootDir": "ts",
    "resolveJsonModule": true,
    "outDir": "../../build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true
  }
}